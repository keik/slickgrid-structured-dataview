{
  "name": "slickgrid-structured-dataview",
  "version": "0.4.0",
  "description": "Custom DataView to enable SlickGrid to deal with structured data.",
  "main": "dist/slick.structureddataview.js",
  "scripts": {
    "lint": "eslint *.es",
    "test": "npm run mocha-phantomjs",
    "start": "http-server & npm run babel:watch",
    "build": "npm run eslint && npm run babel && npm run uglify && npm run test",
    "-----": "----------------------------------------------------------------",
    "eslint": "eslint ./*.es",
    "mocha-phantomjs": "mocha-phantomjs test/index.html",
    "babel": "rm dist -rf && babel -d dist slick.structured*.es",
    "babel:watch": "babel -d dist slick.structured*.es -w",
    "uglify": "find dist -type f ! -name \"*min.js\" -exec bash -c 'node_modules/.bin/uglifyjs \"$0\" -o \"${0%.js}.min.js\" --comments'  {} \\;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keik/slickgrid-structured-dataview.git"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "plugin"
  ],
  "author": {
    "name": "keik",
    "email": "k4t0.kei@gmail.com",
    "url": "keik.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keik/slickgrid-structured-dataview/issues"
  },
  "homepage": "https://github.com/keik/slickgrid-structured-dataview#readme",
  "devDependencies": {
    "babel": "^5.8.20",
    "chai": "^3.2.0",
    "eslint": "^1.0.0",
    "http-server": "^0.8.0",
    "mocha-phantomjs": "^3.6.0",
    "uglify-js": "^2.4.24"
  }
}
