{
  "name": "slickgrid-structured-dataview",
  "version": "0.1.0",
  "description": "Custom DataView to enable SlickGrid to deal with structured data.",
  "main": "dist/slick.structureddataview.js",
  "scripts": {
    "lint": "eslint slick.structureddataview.js || true",
    "test": "npm run lint && npm run babel && npm run mocha-phantomjs 2>/dev/null",
    "build": "npm run test && npm run uglify",
    "start": "http-server & npm run babel:watch",
    "uglify": "uglifyjs dist/slick.structureddataview.js --comments -o dist/slick.structureddataview.js",
    "mocha-phantomjs": "mocha-phantomjs test/index.html",
    "babel": "babel -d dist slick.structureddataview.js",
    "babel:watch": "babel -d dist slick.structureddataview.js -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keik/slickgrid-structured-dataview.git"
  },
  "keywords": [
    "slickgrid",
    "grid",
    "plugin"
  ],
  "author": {
    "name": "keik",
    "email": "k4t0.kei@gmail.com",
    "url": "keik.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keik/slickgrid-structured-dataview/issues"
  },
  "homepage": "https://github.com/keik/slickgrid-structured-dataview#readme",
  "devDependencies": {
    "babel": "^5.8.20",
    "chai": "^3.2.0",
    "eslint": "^1.0.0",
    "http-server": "^0.8.0",
    "mocha-phantomjs": "^3.6.0",
    "uglify-js": "^2.4.24"
  }
}
