// JSHint@2.4.4

{
  "devel": true,

  // Enforcing options
  "camelcase": true,
  "curly": false,
  "eqeqeq": true,
  "eqnull": true,
  "globalstrict": false,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "plusplus": false,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "trailing": true,
  "maxparams": false,
  "maxdepth": false,
  "maxstatements": false,
  "maxcomplexity": false,
  "maxlen": false,

  // Relaxing options
  // none

  // Environment
  "browser": true,
  "jquery": true,

  // Legacy
  "white": true
}
